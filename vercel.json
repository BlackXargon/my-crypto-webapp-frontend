{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*).",
        "headers": { "cache-control": "s-maxage=1, stale-while-revalidate" },
        "dest": "/$1"
      },
      {
        "src": "/",
        "headers": { "cache-control": "s-maxage=1, stale-while-revalidate" },
        "dest": "/index.html"
      }
    ]
  }
  